import random
num=random.randint(1,10)
print("The generated number is ",num)
guess=None
while guess!=num:
	guess=input("guess a numbee between 1 and 10:")
	guess =int(guess)
	if guess==num:
		print("Congratulatins! You Won!")

		break
	else:
		print("Nope , Sry . try again!")
		#print("The generated number is ",guess)
